
/**app.wxss**/
 .container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}  
body{
	padding: 0;
	margin: 0;
}
.yt_container{
	box-sizing:border-box;
	padding: 48rpx;
	width: 100%;
	/* min-height: 800px; */
	background-color: #fff;
  font-family: PingFangSC-Regular, sans-serif;
  /*font-size: 28rpx;*/
}
.fl{float:left}
.fr{float:right}
.clearfix::after {
    content: ".";
    clear: both;
    display: block;
    overflow: hidden;
    font-size: 0;
    height: 0;
}

.clearfix {
    zoom: 1;
}
button::after{
  border: none;
}
.button-hover{
  background-color: #fff;
}
