/* pages/actdetails/mysign/mysign.wxss */
.yt_container{
  margin-bottom:102rpx; 
}
.content{
  position: relative;
  padding:48rpx;
  box-shadow: 4rpx 4rpx 4rpx 4rpx rgba(47,129,212,0.12);
  margin-bottom: 24rpx;
  box-sizing: border-box;
}
.content >.cont_head{
  font-size: 32rpx;
  color: #333;
  font-weight: 700;
  width:68%;
}
.content .cont_ct{
  margin-top: 24rpx;
}
.content .cont_ct .v_icon{
  font-size: 28rpx;
  color:#999;
}
.content .cont_ct .v_icon image{
  width:30rpx;
  height:30rpx;
  vertical-align: middle;
  margin-right: 12rpx;
}
.now{
  font-size: 24rpx;
  position:absolute;
  top:24rpx;
  right:24rpx;
  /* width:160rpx;
  height:40rpx; */
  padding:14rpx 12rpx;
  background-color: #2f81d4;
  border-radius:  28rpx 28rpx 28rpx 0 ;
  color:#fff;
  
}
.del{
  width:188rpx;
  height:66rpx;
  line-height: 66rpx;
  position: absolute;
  font-size: 32rpx;
  font-weight: 700;
  color:#999;
  bottom: -24rpx;
  right:24rpx;
  background-color: #fff;
}
/* 底部部分  */
.foot{
  position: fixed;
  bottom: 0;
  width: 100%;
  background:#fff;
  border-top: 2rpx solid #bbb;
}
.foot>.foot_box{
  width:60%;
  margin: 0 auto;
}
.foot_box>view {
  text-align: center;
  font-size:14px;
}
.foot_box>view image{
  width:64rpx;
  height:64rpx;
}