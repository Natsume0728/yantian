/* pages/photo_intro/photo_intro.wxss */
.photo_box>image{
  float: left;
  width:190rpx;
  height:190rpx;
  margin-right: 36rpx;
  margin-bottom: 36rpx;
}
.photo_box>image:nth-child(3n){
  margin-right: 0rpx;
}
.big{
  margin: 0 auto;
  width:600rpx;
  height:800rpx;
}
.big>image{
  width:100%;
  height:100%;
}
/* 遮罩层部分  */
.mask{
	width:100%;
	height:100%;
	position: fixed;
	background-color:rgba(0, 0, 0, 0.4);
	top:0;
	left:0;
	 
  z-index: 100;
}
.mask_box{
	 width:100%;
   height:100%;   
	position: absolute;
	background-color: #000;
	top:50%;
	left:50%;
	transform: translate(-50%, -50%);
  font-size: 28rpx;
  color:#333;
  text-align: center;
  padding: 0;
  margin: 0 auto;
  overflow: hidden;
}
.mask_box image{
  width:100%;
  height:100%;
  position: absolute;
	top:50%;
	left:50%;
	transform: translate(-50%, -50%);
}
